  {% extends 'base.html.twig' %}

{% block stylesheets %}
    <style type="text/css">
        .all { justify-content: center; }
        .block {  margin: 5% 10% 0% 10%; height: 200px;}
        h2, p, a{ text-align: center;}
    </style>
{% endblock %}

{% block body %}
<div class="block">
    <h1>Ajouter un fichier CSV</h1>

    {{ form_start(form) }}
        {{ form_row(form.FileCsv) }}
        
        <button type="submit" class="btn btn-success">Enregistrer</button>
    {{ form_end(form) }}
</div>

{% endblock %}